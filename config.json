{
	"livereload": 35729,
	"library": {
		"folders": "../library/wavs"
	},
	"createdirs": [
		"../inbox",
		"../library/mogg",
		"../library/mwav",
		"../library/meta",
		"../library/wavs"
	],
	"ingest": {
		"input": "../inbox/**/*.mogg",
		"output": "../library/mogg/<%= filename %>"
	},
	"oggdec": {
		"input": "../library/mogg/**/*.mogg",
		"output": "../library/mwav/<%= folder %>.wav"
	},
	"meta": {
		"input": "../library/mwav/**/*.wav",
		"output": "../library/meta/<%= folder %>.json"
	},
	"explode": {
		"input": "../library/mwav/**/*.wav",
		"output": "../library/wavs/<%= folder %>/<%= tracknum %>.<%= folder %>.wav"
	}
}
